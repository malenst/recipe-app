<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MyEditText" parent="@style/Widget.AppCompat.EditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">55dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginEnd">16dp</item>
        <item name="android:background">@drawable/searchview</item>
        <item name="android:focusable">true</item>
        <item name="android:fontFamily">@font/roboto_light</item>
        <item name="android:paddingStart">24dp</item>
        <item name="android:paddingEnd">16dp</item>
        <item name="android:textColorHint">#5A5858</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">17sp</item>
    </style>
</resources>